# ================random template=======================
from random import randint

def random_str(flag):
    '''
    flag = '01'
    flag = 'az'
    flag = 'Az'
    flag = 'AZ'
    '''
    s = ''
    n = randint(1,20)
    if flag == 'Az':
        for _ in range(n):
            ok = randint(0,1)
            c1 = randint(ord('A'),ord('Z'))
            c2 = randint(ord('a'),ord('z'))
            s += chr(c1 if ok else c2)
    else:
        st = ord(flag[0])
        ed = ord(flag[1])
        for _ in range(n):
            s += chr(randint(st,ed))
    return s


def random_array_int(min_v,max_v):
    '''
    '''
    n = randint(1,20)
    a = [0] * n
    for _ in range(n):
        a[_] = randint(min_v,max_v)
    return a


def random_array_str(flag):
    '''
    '''
    n = randint(1,20)
    a = []
    for _ in range(n):
        a[_] = random_str(flag)
    return a

# ================random template=======================

def solve():
    pass

tt=1
# tt=II()
for _ in range(tt):
    solve()



